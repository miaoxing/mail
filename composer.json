{
  "name": "miaoxing/mail",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Mail Plugin",
  "keywords": [
    "miaoxing",
    "mail"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.5.1",
    "phpmailer/phpmailer": "5.2.*"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Mail\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Mail\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "require-ci": {
      "miaoxing/dev": "miaoxing/dev as 8.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 0.1.x-dev"
    }
  },
  "require-dev": {
    "miaoxing/dev": "^8.1.0"
  }
}
