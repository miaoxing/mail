{
  "name": "miaoxing/mail",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Mail Plugin",
  "keywords": [
    "miaoxing",
    "mail"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.2.13",
    "phpmailer/phpmailer": "5.2.*"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Mail\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Mail\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-types": [
      "miaoxing-plugin"
    ],
    "installer-paths": {
      "plugins/{$name}/": [
        "type:miaoxing-plugin"
      ]
    }
  }
}
